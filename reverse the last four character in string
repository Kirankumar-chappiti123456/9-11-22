#include <stdio.h>
#include<string.h>
#define N 4
int main() {
   char str[20];
 
   printf("enter the string:"); 
   scanf("%s",str);
  int  l=strlen(str);
   for(int l=0;str[l]!='\0';l++)
   {
       for(int i=0;i<N/2;i++)
       {
           
        char temp=str[l-N+i];
        str[l-N+i]=str[l+i-1];
        str[l+i-1]=temp;
       }
   }
    for(int i=0;i<l;i++)
   {
   printf("%c",str[i]);
   }
}

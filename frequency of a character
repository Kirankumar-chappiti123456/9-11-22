#include <stdio.h>
int main() {
    char str[1000]="kirankumar", ch;
    int count = 0;
    printf("Enter a character to find the frequency: ");
    scanf("%c", &ch);

    for (int i = 0; str[i] != '\0'; i++) {
        if (ch == str[i])
            count++;
    }
printf("Frequency of %c = %d", ch, count);
}
